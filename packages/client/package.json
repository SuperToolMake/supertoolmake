{
  "name": "@budibase/client",
  "version": "0.0.0",
  "license": "MPL-2.0",
  "module": "dist/budibase-client.js",
  "main": "dist/budibase-client.js",
  "type": "module",
  "svelte": "src/index.ts",
  "exports": {
    ".": {
      "svelte": "./src/index.ts",
      "import": "./dist/budibase-client.js",
      "require": "./dist/budibase-client.js"
    },
    "./package.json": "./package.json",
    "./manifest.json": "./manifest.json"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch --mode=dev",
    "check:types": "yarn svelte-check",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@budibase/bbui": "*",
    "@budibase/frontend-core": "*",
    "@budibase/shared-core": "*",
    "@budibase/string-templates": "*",
    "@budibase/types": "*",
    "@spectrum-css/card": "3.0.3",
    "apexcharts": "^3.48.0",
    "dayjs": "^1.10.8",
    "downloadjs": "1.4.7",
    "sanitize-html": "^2.13.0",
    "shortid": "^2.2.15",
    "svelte-legacy": "npm:svelte@4.2.20",
    "svelte-spa-router": "^4.0.1"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.1.3",
    "vite": "7.1.11",
    "vite-plugin-css-injected-by-js": "3.5.2",
    "vitest": "^3.2.4"
  },
  "resolutions": {
    "loader-utils": "1.4.1"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          {
            "projects": [
              "@budibase/string-templates"
            ],
            "target": "build"
          }
        ]
      },
      "dev": {
        "dependsOn": [
          {
            "projects": [
              "@budibase/string-templates"
            ],
            "target": "build"
          }
        ]
      }
    }
  }
}
